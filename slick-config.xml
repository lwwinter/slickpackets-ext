<slick-packets>
	<network id="TestSlick">
		<hosts-list>
			<host id="RTR1" type="router" queue-size="10" throughput="20"/>
			<host id="RTR2" type="router" queue-size="15" throughput="10"/>
			<host id="RTR3" type="router" queue-size="40" throughput="40"/>
			<host id="RTR4" type="router" queue-size="45" throughput="50"/>
			<host id="END1" type="endhost"/>
			<host id="END2" type="endhost"/>
		</hosts-list>
		<links-list>
			<link id="LNK1" type="SimpleLink" bandwidth="100" latency="10" from="RTR1" to="RTR2" />
			<link id="LNK2" type="SimpleLink" bandwidth="100" latency="20" from="RTR1" to="RTR3" />
			<link id="LNK3" type="SimpleLink" bandwidth="100" latency="50" from="RTR1" to="RTR4" />
			<link id="LNK4" type="SimpleLink" bandwidth="100" latency="10" from="RTR2" to="RTR4" />
			<link id="LNK5" type="SimpleLink" bandwidth="100" latency="50" from="RTR3" to="RTR4" />
			<link id="LNK6" type="SimpleLink" bandwidth="100" latency="10" from="END1" to="RTR4" />
			<link id="LNK7" type="SimpleLink" bandwidth="100" latency="10" from="END2" to="RTR3" />
		</links-list>
	</network>
</slick-packets>